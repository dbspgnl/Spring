<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="lee" class="com.test03.Address">
		<property name="name" value="이순신"></property> <!-- 필드가 아니라 setter를 불러오는 것 -->
		<property name="addr" value="서울시 강남구"/>
		<property name="phone" value="010-1234-5678"/>
	</bean>
	
	<bean id="hong" class="com.test03.Address">
		<property name="name" value="홍길동"/>
		<property name="addr" value="경기도 수원시"/>
		<property name="phone" value="010-1234-1234"/>
	</bean>
</beans>
